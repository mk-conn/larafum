{{#link-to "index.feed.articles" feed.id}}
  <div class="">
      <span class="feed-icon">
        {{#if feed.icon}}
          <img src="{{feed.icon}}" alt="feed icon">
        {{else}}
          <img src="/frontend/img/feed-icon.png" alt="feed icon">
        {{/if}}
      </span>
    <span class="">{{feed.name}}</span>
    {{#if feed.unreadCount}}
      <span class="badge badge-secondary">{{feed.unreadCount}}</span>
    {{/if}}
  </div>
{{/link-to}}

