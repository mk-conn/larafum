<div class="panel panel-default">
  <div class="panel-heading">
    {{model.feed.name}}
  </div>
  <div class="panel-body">
    <p>{{model.feed.description}}</p>
  </div>

  <div class="list-group">
    {{#each model.articles as |article|}}
      {{article-item article=article tagName="hbox" classNames="list-group-item article-item"}}
    {{/each}}
  </div>
</div>

{{outlet}}
