{
  "name": "mk-conn/nextfeed2",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.0.0",
    "cloudcreativity/laravel-json-api": "v1.0.0-beta.5",
    "doctrine/dbal": "2.6.*",
    "fideloper/proxy": "^4.0",
    "laravel/framework": "5.7.*",
    "laravel/scout": "6.1.*",
    "laravel/tinker": "1.0.*",
    "zendframework/zend-feed": "2.9.*",
    "mk-conn/shellax": "0.3.*",
    "pmatseykanets/laravel-scout-postgres": "4.0.*",
    "laravel/passport": "7.0.*",
    "ext-dom": "*"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.4",
    "beyondcode/laravel-dump-server": "^1.0",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^2.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "Laravel\\Passport\\PassportServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
