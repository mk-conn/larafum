{
  "name": "mk-conn/nextfeed2",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "authors": [
    {
      "name": "Marko KrÃ¼ger",
      "email": "nextfeed@marko-krueger.de",
      "role": "Lead Developer"
    }
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1.0",
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-simplexml": "*",
    "ext-tidy": "*",
    "cloudcreativity/laravel-json-api": "1.1.*",
    "cloudcreativity/json-api-testing": "1.1.*",
    "doctrine/dbal": "^2.9",
    "xemlock/htmlpurifier-html5": "0.1.*",
    "fideloper/proxy": "^4.0",
    "laravel/framework": "5.8.*",
    "laravel/passport": "7.3.*",
    "laravel/scout": "7.1.*",
    "laravel/tinker": "1.0.*",
    "masterminds/html5": "2.6.*",
    "mk-conn/shellax": "0.3.*",
    "owen-it/laravel-auditing": "9.2.*",
    "pmatseykanets/laravel-scout-postgres": "5.0.*",
    "predis/predis": "1.1.*",
    "zendframework/zend-feed": "2.10.*",
    "zendframework/zend-http": "2.9.*"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "2.6.*",
    "beyondcode/laravel-dump-server": "1.2.*",
    "filp/whoops": "2.3.*",
    "fzaninotto/faker": "1.8.*",
    "mockery/mockery": "1.2.*",
    "nunomaduro/collision": "3.0.*",
    "phpunit/phpunit": "8.1.*"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "Laravel\\Passport\\PassportServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
